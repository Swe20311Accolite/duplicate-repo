pipeline {
  agent any
  stages {
    stage('checkout-repo') {
      steps {
        sh 'mkdir repo'
        dir(path: 'repo') {
          git(url: 'https://github.com/Swe20311Accolite/cluster-repo.git', branch: 'main')
        }

      }
    }

    stage('testing') {
      steps {
        kubeconfig(serverUrl: 'https://192.168.49.2:8443', caCertificate: '', credentialsId: 'b9527f91-e186-42ce-84af-0e3d530ae39f') {
          sh 'kubectl get all'
        }

      }
    }

  }
}